<Window
    Title="CodeTest_UnitTestWindow"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="UnitTestTool.CodeTest_UnitTestWindow"
    xmlns="https://github.com/avaloniaui"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Grid
        Background="{DynamicResource Theme_BackgroundBrush}"
        Height="720"
        Width="1080">
        <Grid
            Background="#FFF0F0F0"
            Height="32"
            HorizontalAlignment="Left"
            Margin="8,16,0,0"
            VerticalAlignment="Top"
            Width="696">
            <Button
                Content="Start"
                Height="32"
                HorizontalAlignment="Left"
                Margin="41,0,0,0"
                VerticalAlignment="Top"
                Width="40" />
            <Button
                Content="Stop"
                Height="32"
                HorizontalAlignment="Left"
                Margin="82,0,0,0"
                VerticalAlignment="Top"
                Width="53" />
        </Grid>
    </Grid>
</Window>
